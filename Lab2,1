#include <stdlib.h>
#include <stdio.h>
#include <math.h>
int main()
{
 double x,y;

 printf("Enter x:");
 scanf("%lf", &x);

   if( -10 <= x && x < -2)
          y = - pow(x, 3) + 1;
   else if ( 3 <= x && x < 8 )
          y = 1 / (2 * x);
    else if ( 12 <= x && x < 20 )
          y = pow(x, 3) / sqrt(x - 1);
       else
        y = 5;

 system("cls");
 printf("x = %lf", x);
 printf("\ny = %lf", y);
 return 0;
}
